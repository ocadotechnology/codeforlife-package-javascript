{
  "name": "codeforlife",
  "description": "Common frontend code",
  "private": false,
  "version": "2.7.2",
  "type": "module",
  "scripts": {
    "cli": "VITE_CONFIG=./vite.config.ts ../scripts/frontend $@"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ocadotechnology/codeforlife-package-javascript.git"
  },
  "author": "Ocado",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/ocadotechnology/codeforlife-package-javascript/issues"
  },
  "homepage": "https://github.com/ocadotechnology/codeforlife-package-javascript#readme",
  "exports": {
    ".": "./src/index.ts",
    "./api": "./src/api/index.ts",
    "./api/*": "./src/api/*.ts",
    "./components": "./src/components/index.ts",
    "./components/form": "./src/components/form/index.tsx",
    "./components/page": "./src/components/page/index.ts",
    "./components/router": "./src/components/router/index.tsx",
    "./components/table": "./src/components/table/index.tsx",
    "./components/*": "./src/components/*.tsx",
    "./features": "./src/features/index.ts",
    "./features/*": "./src/features/*.tsx",
    "./fonts/*": "./src/fonts/*.ttf",
    "./hooks": "./src/hooks/index.ts",
    "./hooks/*": "./src/hooks/*.tsx",
    "./images/svg/*": "./src/images/svg/*.svg",
    "./middlewares": "./src/middlewares/index.ts",
    "./middlewares/*": "./src/middlewares/*.ts",
    "./settings": "./src/settings/index.ts",
    "./settings/*": "./src/settings/*.ts",
    "./slices": "./src/slices/index.ts",
    "./slices/*": "./src/slices/*.ts",
    "./theme": "./src/theme/index.ts",
    "./theme/*": "./src/theme/*.ts",
    "./utils/api": "./src/utils/api.tsx",
    "./utils/test": "./src/utils/test.tsx",
    "./utils/theme": "./src/utils/theme.tsx",
    "./utils/*": "./src/utils/*.ts",
    "./server": "./src/server.js"
  },
  "dependencies": {
    "@emotion/react": "^11.10.6",
    "@emotion/styled": "^11.10.6",
    "@mui/icons-material": "^5.11.11",
    "@mui/material": "^5.11.12",
    "@mui/x-date-pickers": "^7.7.1",
    "@reduxjs/toolkit": "^2.0.1",
    "compression": "^1.7.5",
    "dayjs": "^1.11.11",
    "express": "^4.21.2",
    "formik": "^2.2.9",
    "js-cookie": "^3.0.5",
    "memory-cache": "^0.2.0",
    "qs": "^6.11.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-redux": "^9.1.0",
    "react-router-dom": "^6.23.1",
    "sirv": "^3.0.0",
    "yup": "^1.1.1"
  },
  "devDependencies": {
    "@testing-library/dom": "^9.3.4",
    "@testing-library/jest-dom": "^6.2.0",
    "@testing-library/react": "^14.1.2",
    "@testing-library/user-event": "^14.5.2",
    "@types/express": "^5.0.0",
    "@types/js-cookie": "^3.0.3",
    "@types/node": "^22.14.1",
    "@types/qs": "^6.9.7",
    "@types/react": "^18.2.47",
    "@types/react-dom": "^18.2.18",
    "@vitejs/plugin-react": "^4.2.1",
    "@vitest/coverage-istanbul": "^1.6.0",
    "@vitest/ui": "^1.6.0",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-config-react-app": "^7.0.1",
    "eslint-plugin-prettier": "^5.1.3",
    "jsdom": "^23.2.0",
    "prettier": "^3.2.1",
    "typescript": "^5.3.3",
    "vite": "^5.0.11",
    "vitest": "^1.2.0"
  },
  "release": {
    "branches": [
      "main"
    ],
    "plugins": [
      "@semantic-release/commit-analyzer",
      "@semantic-release/release-notes-generator",
      "@semantic-release/changelog",
      "@semantic-release/npm",
      [
        "@semantic-release/git",
        {
          "assets": [
            "package.json",
            "CHANGELOG.md"
          ],
          "message": "chore(release): ${nextRelease.version} [skip ci]\n\n${nextRelease.notes}"
        }
      ],
      "@semantic-release/github"
    ]
  }
}
