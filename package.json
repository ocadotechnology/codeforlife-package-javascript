{
  "name": "codeforlife",
  "version": "1.27.5",
  "private": true,
  "description": "Common frontend code",
  "main": "./lib/cjs/index.js",
  "module": "./lib/esm/index.js",
  "types": "./lib/esm/index.d.ts",
  "files": [
    "/lib"
  ],
  "scripts": {
    "lint": "eslint src",
    "test": "echo \"Error: no test specified\" && exit 1",
    "build": "rm -rf lib && npm run build:esm && npm run build:cjs && npm run copy",
    "build:esm": "tsc",
    "build:cjs": "tsc --module commonjs --outDir lib/cjs",
    "copy": "npm run copy:esm && npm run copy:cjs",
    "copy:esm": "cp -r src/public lib/esm/public",
    "copy:cjs": "cp -r src/public lib/cjs/public"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ocadotechnology/codeforlife-package-javascript.git"
  },
  "author": "Ocado",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/ocadotechnology/codeforlife-package-javascript/issues"
  },
  "homepage": "https://github.com/ocadotechnology/codeforlife-package-javascript#readme",
  "dependencies": {
    "@emotion/react": "^11.10.6",
    "@emotion/styled": "^11.10.6",
    "@mui/icons-material": "^5.11.11",
    "@mui/material": "^5.11.12",
    "@reduxjs/toolkit": "^2.0.1",
    "formik": "^2.2.9",
    "js-cookie": "^3.0.5",
    "qs": "^6.11.2",
    "yup": "^1.1.1"
  },
  "devDependencies": {
    "@testing-library/dom": "^9.3.4",
    "@testing-library/jest-dom": "^6.2.0",
    "@testing-library/react": "^14.1.2",
    "@testing-library/user-event": "^14.5.2",
    "@types/js-cookie": "^3.0.3",
    "@types/node": "^20.2.5",
    "@types/qs": "^6.9.7",
    "@types/react": "^18.2.47",
    "@types/react-dom": "^18.2.18",
    "@vitejs/plugin-react": "^4.2.1",
    "@vitest/coverage-istanbul": "^1.6.0",
    "@vitest/ui": "^1.6.0",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-config-react-app": "^7.0.1",
    "eslint-plugin-prettier": "^5.1.3",
    "jsdom": "^23.2.0",
    "prettier": "^3.2.1",
    "typescript": "^5.3.3",
    "vite": "^5.0.11",
    "vitest": "^1.2.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-redux": "^9.1.0",
    "react-router-dom": "^6.8.2"
  },
  "peerDependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-redux": "^9.1.0",
    "react-router-dom": "^6.8.2"
  },
  "release": {
    "branches": [
      "main"
    ],
    "plugins": [
      "@semantic-release/commit-analyzer",
      "@semantic-release/release-notes-generator",
      "@semantic-release/changelog",
      "@semantic-release/npm",
      [
        "@semantic-release/git",
        {
          "assets": [
            "package.json",
            "CHANGELOG.md"
          ],
          "message": "chore(release): ${nextRelease.version} [skip ci]\n\n${nextRelease.notes}"
        }
      ],
      "@semantic-release/github"
    ]
  }
}
